{
  "name": "moovit-client",
  "version": "1.0.0",
  "description": "TypeScript client library for the Moovit public transit API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "moovit",
    "transit",
    "public-transport",
    "api-client",
    "israel"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "protobufjs": "^7.5.4",
    "puppeteer": "^24.36.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  }
}
